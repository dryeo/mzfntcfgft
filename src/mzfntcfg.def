LIBRARY mzfntcfg INITINSTANCE TERMINSTANCE
DESCRIPTION 'mzfntcfg, a new OS/2 Fontconfig version from Netlabs, v1.0'
EXPORTS
     _FcInit
     _FcFini
     _FcPatternCreate
     _FcPatternDestroy
     _FcConfigSubstitute
     _FcDefaultSubstitute
     _FcPatternGetInteger
     _FcPatternGetString
     _FcPatternGetBool
     _FcPatternGet
     _FcPatternAddInteger
     _FcPatternAddString
     _FcPatternAddBool
     _FcFontMatch
     _FcPatternDel

     ; dummies to make cairo testsuite linking possible
     ;_FcFreeTypeQuery
     _FcPatternAddDouble
     _FcPatternGetDouble

     ; dummies to make Mozilla compilation happy
     _FcFontList
     _FcFontSort
     _FcObjectSetBuild
     _FcConfigUptoDate
     _FcFontSetDestroy
     _FcInitReinitialize
     _FcObjectSetDestroy
